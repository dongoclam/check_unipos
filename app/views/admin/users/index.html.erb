<div class="col-md-12">
   <div class="card">
      <div class="card-header card-header-info">
         <h4 class="card-title ">Users <strong>(<%= @count %>)</strong></h4>
         <p class="card-category">All users are from Framgia HN</p>
         <%= button_tag id: "clone-users", class: "btn btn-primary btn-simple" do %>
           <i class="material-icons">cloud_download</i> Clone
         <% end %>
         <%= button_tag id: "update-users", class: "btn btn-primary btn-simple" do %>
           <i class="material-icons ">autorenew</i> Update
         <% end %>
      </div>
      <div class="card-body">
         <div class="table-responsive">
           <%= render "table" %>
         </div>
      </div>
   </div>
</div>

<%= render "shared/modal_detail" %>
<%= render "shared/modal_confirm" %>
<%= render "shared/modal_loading" %>
