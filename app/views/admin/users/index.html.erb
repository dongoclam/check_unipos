<div class="col-md-12">
   <div class="card">
      <div class="card-header card-header-info">
         <h4 class="card-title ">Unipos Users</h4>
         <p class="card-category">All users are from Framgia HN</p>
         <button type="button" class="btn btn-primary load-user-from-unipos">
           <i class="material-icons">cloud_download</i> Clone
         </button>
         <button type="button" class="btn btn-primary update-user-information">
           <i class="material-icons ">autorenew</i> Update
         </button>
      </div>
      <div class="card-body">
         <div class="table-responsive">
           <%= render "table" %>
         </div>
      </div>
   </div>
</div>

<!-- Modal -->
<%= render "modal_detail" %>
<%= render "shared/modal_confirm" %>
<%= render "shared/modal_loading" %>
